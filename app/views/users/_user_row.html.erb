<tr id="user-row-<%=user.id%>}" class="bg-white border-b hover:bg-gray-100">
  <td class="px-3 py-3"><%= check_box_tag '', user.id, false, class: 'delete_multiple_checkbox', name: "users[]" %></td>
  <td class="px-3 py-3"><%= user.firstName %></td>
  <td class="px-3 py-3"><%= user.lastName %></td>
  <td class="px-3 py-3"><%= user.email %></td>
  <td class="px-3 py-3"><%= user.gender %></td>
  <td class="px-3 py-3"><%= user.height %></td>
  <td class="px-3 py-3"><%= link_to 'View', user, data: { turbo_frame: "_top" }%></td>
  <td class="px-3 py-3"><%= link_to 'Edit', edit_user_path(user), data: { turbo_frame: "_top" }%></td>
</tr>
